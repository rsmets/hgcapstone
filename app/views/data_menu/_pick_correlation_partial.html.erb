<h2>Pick a data set</h2>

<%= link_to "/data_correlations",:target => "_blank" do %> 
   <%= label_tag(:q, "View Correlation Results") %>
   <% end %>

<%= form_tag('/data_menu/pick_range', method: 'get') do %>
	<ol>
	<%= @dt.each do |c| %>
		<li>
		<%= check_box_tag(" ") %>
		<%= c.name %>
		</li>
	<% end %>
	</ol>
	<%= submit_tag("Graph it!") %>
<% end %>	