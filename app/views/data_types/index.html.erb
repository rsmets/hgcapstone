<p id="notice"><%= notice %></p>

<h1>Listing Data Types</h1>

<table>
  <thead>
    <tr>
      <th>(Name)</th>
      <th>(Data Type ID)</th>
      <th>(Url)</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_types.each do |data_type| %>
      <tr>
        <td><%= data_type.name %></td>
        <td><%= data_type.id %></td>
        <td><%= data_type.url %></td>
        <td><%= link_to 'Show', data_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data type', new_data_type_path %>
