
<%= javascript_include_tag "graph.js" %>

<%= form_tag("/graph", method: "get", id: "graphForm") do %>
		<%= label_tag(:q, "Enter year range:") %>
		<%= number_field_tag(:num) %>
		<%= number_field_tag(:num2) %>
		<%= submit_tag("Search") %>
		<%= @selected_dt %>
		<%= @selected_dt2 %>
		<%= @num %>
		<%= @num2 %>
<% end %>

